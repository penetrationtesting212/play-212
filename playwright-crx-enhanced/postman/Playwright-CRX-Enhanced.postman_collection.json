          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/self-healing/suggestions/{{scriptId}}?status=pending",
              "host": ["{{baseUrl}}"],
              "path": ["self-healing", "suggestions", "{{scriptId}}"],
              "query": [
                {
                  "key": "status",
                  "value": "pending"
