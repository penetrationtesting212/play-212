    {
      "name": "Data-Driven Testing (DDT)",
      "item": [
        {
          "name": "Upload CSV File",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": "/path/to/test-data.csv"
                },
                {
                  "key": "scriptId",
                  "value": "{{scriptId}}",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/test-data/upload",
