{
  "name": "Playwright CRX Tests",
  "version": "0.15.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "CRX Tests",
    "default_popup": "popup.html"
  },
  "permissions": ["debugger", "tabs", "storage"],
  "host_permissions": [
    "http://127.0.0.1:3001/*",
    "http://localhost:3001/*"
  ]
}
